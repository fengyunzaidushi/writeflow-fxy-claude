{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/**/*.ts",
    "scripts/*.mjs"
  ],
  "project": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "ignore": [
    "dist/**",
    "node_modules/**",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/__tests__/**",
    "**/mock/**",
    "**/mocks/**"
  ]
}